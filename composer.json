{
	"name": "coullc/tusk",
	"description": "Tusk, a CMS and Application-Framework plugin for CakePHP",
	"type": "cakephp-plugin",
	"keywords": [
		"cakephp",
		"cms"
	],
	"homepage": "https://rhino.coull.dev",
	"license": "MIT",
	"authors": [
		{
			"name": "Carsten Coull",
			"homepage": "https://coull.dev",
			"role": "Author"
		}
	],
	"require": {
		"php": ">=7.4",
		"cakephp/authentication": "^2.0",
		"cakephp/authorization": "^2.0",
		"cakephp/cakephp": "^4.4",
		"cakephp/migrations": "^3.8",
		"cakephp/plugin-installer": "^1.3",
		"friendsofcake/cakephp-csvview": "^2.1"
	},
	"require-dev": {
		"phpunit/phpunit": "^8.5 || ^9.3"
	},
	"autoload": {
		"psr-4": {
			"Tusk\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tusk\\Test\\": "tests/",
			"Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
		}
	},
	"minimum-stability": "dev",
	"config": {
		"allow-plugins": {
			"cakephp/plugin-installer": true,
			"composer/installers": true
		}
	}
}
